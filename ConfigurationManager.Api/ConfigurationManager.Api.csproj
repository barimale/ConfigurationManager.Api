<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <AssemblyVersion>1.3.3.1</AssemblyVersion>
    <FileVersion>1.3.3.1</FileVersion>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.3.3.1</Version>
    <Authors>barimale</Authors>
    <Description>.NET Consul Client provided with eager and lazy appSettings and connectionStrings adapters</Description>
    <RepositoryUrl>https://github.com/barimale/ConfigurationManager.Api</RepositoryUrl>
    <PackageTags>Consul</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Consul" Version="0.7.2.6" />
    <PackageReference Include="Ninject.Extensions.Factory" Version="3.3.2" />
    <PackageReference Include="System.Configuration.ConfigurationManager" Version="4.7.0" />
  </ItemGroup>

</Project>
